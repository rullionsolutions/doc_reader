{
  "name": "dox",
  "version": "0.1.0",
  "description": "Local Browse Functionality on a Git Repo containing Markdown",
  "main": "index.js",
  "scripts": {
    "build": "tsc; browserify src/App.js -t [ babelify --presets [ env react ] ] | uglifyjs --compress unused - > dox.min.js",
    "start": "node webserver.js",
    "test": "nodeunit test.js",
    "watch": "watchify src/App.tsx -p [ tsify ] -t [ babelify --presets [ env react ] ] -o dox.min.js --debug --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:stephenfrancis/dox"
  },
  "author": "stephen.d.francis@gmail.com",
  "license": "Apache-2.0",
  "dependencies": {
    "finalhandler": "^1.1.0",
    "serve-static": "^1.13.1"
  },
  "devDependencies": {
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.38",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "eslint": "^4.6.1",
    "eslint-plugin-react": "^7.5.1",
    "lapis": "https://github.com/stephenfrancis/lapis.git",
    "loglevel": "^1.6.0",
    "marked": ">= 0.3.6",
    "prop-types": "^15.6.0",
    "q": ">= 1.4.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "tsify": "^3.0.4",
    "typescript": "^2.7.1",
    "uglify-js": "^2.4.11",
    "viz.js": ">= 1.4.1",
    "watchify": "^3.9.0"
  }
}
